<hibernate-mapping package="ru.ithex.model">
	<class name="Employment" table="oltp.employment">
		<id name="employmentID" column="ID" type="java.lang.Integer">
			<generator class="increment" />
		</id>
		<property name="position" column="position" type="java.lang.Integer" />
		<property name="incomeAmount" column="income_amount" type="java.math.BigDecimal" />
		<property name="startWorkingDate" column="start_working_date" type="java.util.Date" />
		<property name="endWorkingDate" column="end_working_date" type="java.util.Date" />	
		<many-to-one name="organization" column="organization_fk" class="Organization" />
	</class>	
</hibernate-mapping>