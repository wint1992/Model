<hibernate-mapping package="ru.ithex.model">
	<class name="Product" table="oltp.Product">
		<id name="productID" column="product_ID" type="java.lang.Integer">
			<generator class="increment" />
		</id>
		<property name="productName" column="product_Name" type="java.lang.String" />
		<property name="productCost" column="product_Cost" type="java.math.BigDecimal" />
		<property name="productType" column="product_Type" type="java.lang.Integer" />
		<property name="productGroup" column="product_Group" type="java.lang.Integer" />
		<property name="productSubgroup" column="product_Subgroup" type="java.lang.Integer" />
		<property name="productSpecies" column="product_Species" type="java.lang.Integer" />
		<property name="productVariety" column="product_Variety" type="java.lang.Integer" />
		<property name="productBrand" column="product_Brand" type="java.lang.String" />
		<property name="productCount" column="product_Count" type="java.lang.Integer" />
		<property name="productWeight" column="product_Weight" type="java.math.BigDecimal" />
		<property name="productLength" column="product_Length" type="java.math.BigDecimal" />
		<property name="productVolume" column="product_Volume" type="java.math.BigDecimal" />
	</class>	
</hibernate-mapping>