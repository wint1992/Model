<hibernate-mapping package="ru.ithex.model">
	<class name="NextStep" table="oltp.next_step">
		<id name="nextStepID" column="ID" type="java.lang.Integer">
			<generator class="increment" />
		</id>
		<property name="stepCode" column="STEP_CODE" type="java.lang.Integer" />
		<property name="stepParameter" column="STEP_PARAMETER" type="java.lang.Integer" />
		<property name="stepPriority" column="STEP_PRIORITY" type="java.lang.Integer" />
		<property name="stepPlaneDate" column="STEP_PLANE_DATE" type="java.util.Date" />
	</class>	
</hibernate-mapping>